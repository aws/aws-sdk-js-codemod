{
  "name": "aws-sdk-js-codemod",
  "version": "0.28.2",
  "description": "Collection of codemod scripts that help update AWS SDK for JavaScript APIs",
  "keywords": [
    "jscodeshift",
    "transform",
    "aws",
    "sdk",
    "javascript",
    "v2",
    "v3",
    "migration"
  ],
  "homepage": "https://github.com/awslabs/aws-sdk-js-codemod",
  "license": "MIT-0",
  "author": {
    "name": "Amazon Web Services",
    "url": "http://aws.amazon.com"
  },
  "contributors": [
    "Kamat, Trivikram <trivikr.dev@gmail.com>"
  ],
  "files": [
    "bin",
    "dist"
  ],
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": "bin/aws-sdk-js-codemod",
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/aws-sdk-js-codemod.git"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "release": "yarn build && changeset publish",
    "test": "vitest"
  },
  "dependencies": {
    "jscodeshift": "0.15.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.21.0",
    "@tsconfig/node14": "^1.0.3",
    "@types/jscodeshift": "^0.11.5",
    "@types/node": "^14.18.33",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "aws-sdk": "2.1495.0",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.28.1",
    "lint-staged": "^13.0.3",
    "prettier": "3.0.3",
    "simple-git-hooks": "^2.8.1",
    "tsx": "^3.12.1",
    "typescript": "~5.2.2",
    "vitest": "~0.34.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@4.0.1",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  }
}
